{
  "name": "freemage",
  "version": "0.1.0",
  "description": "Image sharing app",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "npm start --prefix client",
    "server": "nodemon ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "cloudinary": "^1.14.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "debug": "^2.6.9",
    "express": "^4.16.4",
    "google-auth-library": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2"
  },
  "main": "app.js",
  "devDependencies": {
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1"
  },
  "keywords": [
    "image upload",
    "image stock",
    "photo sharing app",
    "free images",
    "freemage"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shahzayb/freemage.git"
  },
  "author": "Shahzaib Sarwar <imshahzayb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shahzayb/freemage/issues"
  },
  "homepage": "https://github.com/Shahzayb/freemage#readme",
  "engines": {
    "node": "12.8.0",
    "npm": "6.10.2"
  }
}
